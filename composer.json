{
  "name": "bitbirddev/microsoft-teams-monolog-handler",
  "description": "Monolog handler sending Microsoft Teams notifications with an Incoming Webhook",
  "homepage": "https://github.com/bitbirddev/microsoft-teams-monolog-handler",
  "keywords": [
    "microsoft",
    "teams",
    "webhook",
    "notifier",
    "log",
    "monolog"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "bitbirddev",
      "homepage": "https://bitbird.de"
    },
    {
      "name": "Benjamin Schneider"
    }
  ],
  "autoload": {
    "psr-4": {
      "bitbirddev\\MicrosoftTeamsNotifier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "bitbirddev\\MicrosoftTeamsNotifier\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "monolog/monolog": "^3.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5"
  },
  "suggest": {
    "symfony/monolog-bundle": "The MonologBundle provides integration of the Monolog library into the Symfony framework."
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "phpstan": "vendor/bin/phpstan analyse"
  }
}
